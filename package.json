{
  "name": "@augu/immutable",
  "description": "📝 Immutable library in TypeScript",
  "version": "0.1.6",
  "types": "index.d.ts",
  "main": "build/index.js",
  "homepage": "https://auguwu.github.io/immutable",
  "files": ["build/", "esm.mjs", "index.d.ts"],
  "exports": {
    ".": [
      {
        "require": "./build/index.js",
        "import": "./esm.mjs"
      },
      "./build/index.js"
    ],
    "./esm": "./esm.mjs"
  },
  "repository": {
    "url": "https://github.com/auguwu/immutable"
  },
  "devDependencies": {
    "@augu/eslint-config": "1.2.0",
    "@types/node": "12.12.21",
    "@types/jest": "25.1.2",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "eslint": "6.8.0",
    "typedoc": "0.16.10",
    "typescript": "3.7.5",
    "ts-jest": "25.2.0"
  },
  "scripts": {
    "prepare": "yarn docs && yarn build",
    "build": "yarn lint && yarn test && rm -fr build && tsc",
    "lint": "eslint src --ext .ts --fix",
    "test": "jest --config jest.config.js --no-cache",
    "docs": "rm -fr docs && typedoc"
  }
}